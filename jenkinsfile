pipeline {
    agent any

stages    
{
	stage('Build)    
	{
		steps 
		{
			if (isUnix()) 
				{
					sh "'${mvnHome}/bin/mvn' -Dmaven.test.failure.ignore clean package"
				} 
			else 
				{
					echo 'this is build maven artifact'
					bat(/"${mvnHome}\bin\mvn" -Dmaven.test.failure.ignore clean test/)
				}
	    }
	}
}
			}
