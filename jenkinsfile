pipeline {
    agent any

    tools {
        maven 'maven3.6.3'
    }

   envirenment{
	PATH = "C:/Program Files/Java/jdk1.8.0_251/bin:$PATH"
	}
	
    stages {
        stage('clean') {
            steps {
                sh 'mvn -B clean'
            }
        }
        stage('compile') {
            steps {
                sh 'mvn -B compile'
            }
        }
        stage('package') {
            steps {
                sh 'mvn -B package'
            }
        }
    }
}
